#!/bin/bash
rsync -avz ~/.bash_aliases .
rsync -avz  ~/.gitconfig .
rsync -avz ~/.tmux.conf .
rsync -avz --exclude 'elpa' --exclude 'session*' --exclude 'auto-save-list' --exclude 'bak' --exclude '*~' --exclude '*.elc' --exclude '.helm-backup' --exclude '.#*'  --exclude 'recentf'  --exclude 'ido.last' --exclude 'smex-items' ~/.emacs.d/ .emacs.d
